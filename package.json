{
  "name": "miniprogram-component-manager",
  "version": "1.0.0",
  "description": "小程序组件管理系统",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "prebuild": "npm run type-check",
    "build": "webpack --config webpack.config.js",
    "dev": "react-scripts start",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "react-scripts start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "eject": "react-scripts eject",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --watch --config jest.config.cjs",
    "test:cov": "jest --coverage --config jest.config.cjs",
    "test:unit": "jest --config jest.config.js",
    "test:integration": "jest --config jest.config.js --testMatch='**/integration/**/*.test.ts'",
    "test:e2e": "jest --config jest.config.js --testMatch='**/e2e/**/*.test.ts'",
    "test:ci": "jest --config jest.config.cjs --ci --runInBand --coverage",
    "test:clear": "jest --clearCache && rimraf coverage",
    "test:changed": "jest --config jest.config.cjs --onlyChanged",
    "test:debug": "node --inspect-brk jest --runInBand --config jest.config.cjs",
    "pretest": "npm run test:clear",
    "test:watch:unit": "jest --config jest.config.cjs --testMatch \"**/*.spec.ts\" --watch",
    "test:watch:integration": "jest --config jest.config.cjs --testMatch \"**/*.integration.spec.ts\" --watch",
    "test:watch:e2e": "jest --config ./test/jest-e2e.json --watch",
    "test:perf": "jest --config jest.config.cjs --testMatch \"**/*.perf.spec.ts\"",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "fix:code": "ts-node scripts/fix-code-quality.ts",
    "fix:lint": "npm run lint:fix",
    "fix:format": "npm run format",
    "fix:all": "npm run fix:types && npm run fix:lint-all && npm run lint:fix",
    "fix:missing": "ts-node scripts/create-missing-modules.ts",
    "test:watch:changed": "jest --watch --onlyChanged --config jest.config.cjs",
    "test:coverage:watch": "jest --coverage --watch --config jest.config.cjs",
    "test:update": "jest --updateSnapshot --config jest.config.cjs",
    "test:ci:coverage": "jest --ci --coverage --config jest.config.cjs && cat ./coverage/lcov.info | coveralls",
    "health-check": "ts-node scripts/run-health-check.ts",
    "test:db-performance": "jest --config jest.config.js database/mongodb/tests/performance.test.js --passWithNoTests",
    "test:api-performance": "jest --config jest.config.js src/tests/api-performance.test.ts --passWithNoTests",
    "test:memory": "node --inspect scripts/memory-analysis.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "fix-types": "ts-node scripts/fix-type-definitions.ts",
    "update:standards": "ts-node scripts/code-standards-updater.ts",
    "fix:lint-all": "ts-node scripts/fix-lint-errors.ts",
    "analyze": "ts-node scripts/run-health-check.ts --analyze-only",
    "fix:types": "ts-node scripts/fix-type-definitions.ts"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.7",
    "@nestjs/terminus": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nestjs/typeorm": "^8.0.0",
    "@types/react": "^17.0.38",
    "@types/styled-components": "^5.1.0",
    "chalk": "^4.1.2",
    "chart.js": "^3.7.0",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "inversify": "5.0.1",
    "ioredis": "^5.4.1",
    "mongoose": "^8.9.0",
    "morgan": "^1.10.0",
    "prom-client": "^15.1.3",
    "react": "^18.3.1",
    "react-chartjs-2": "^4.0.0",
    "react-dom": "^18.3.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "styled-components": "^5.3.0",
    "styled-jsx": "^5.0.0",
    "ts-morph": "^24.0.0",
    "typeorm": "^0.3.20",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "https-browserify": "^1.0.0",
    "jest": "^29.7.0",
    "mongodb-memory-server-core": "^9.1.6",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.3.2",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "react-scripts": "^5.0.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "main": ".eslintrc.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "pre-commit": [
    "type-check",
    "lint",
    "format"
  ]
}
