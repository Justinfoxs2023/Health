{
  "name": "health-platform",
  "version": "1.0.0",
  "description": "健康管理平台",
  "scripts": {
    "prestart": "node config/.env.config.js",
    "start": "ts-node -r dotenv/config src/main.ts",
    "dev": "cross-env NODE_ENV=development nodemon -r dotenv/config",
    "db:init": "ts-node database/scripts/init-db.ts",
    "db:generate": "ts-node database/scripts/generate-test-data.ts",
    "db:reset": "npm run db:init && npm run db:generate",
    "check:env": "ts-node scripts/check-env.ts",
    "check:env:prod": "cross-env NODE_ENV=production ts-node scripts/check-env.ts",
    "check:env:dev": "cross-env NODE_ENV=development ts-node scripts/check-env.ts",
    "check:env:test": "cross-env NODE_ENV=test ts-node scripts/check-env.ts"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "antd": "^4.24.0",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^10.0.0",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "jest": "^27.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.13.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "redis": "^4.7.0",
    "winston": "^3.8.0",
    "axios": "^1.6.2",
    "@aws-sdk/client-rekognition": "^3.470.0",
    "@aws-sdk/client-s3": "^3.470.0",
    "multer": "^1.4.5-lts.1",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@jest/globals": "^27.0.0",
    "@types/eslint": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.0",
    "@types/lodash": "^4.14.178",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^22.10.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/redis": "^4.0.11",
    "@types/winston": "^2.4.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "nodemon": "^3.1.7",
    "ts-jest": "^27.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "@types/multer": "^1.4.7",
    "@types/aws-sdk": "^2.7.0"
  }
}
